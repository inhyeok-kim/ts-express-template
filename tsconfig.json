{
  "compilerOptions": {
    "target": "ES2016",
    "module": "commonjs",
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "resolveJsonModule" : true,
    "skipLibCheck": true
  },
  "include": [
    "package.json","src","app.ts"
  ],
  "exclude": [
      "node_modules", "src/test"
  ]
}
  